{
    "name": "realcahsowan/laravel-bulma-form",
    "description": "Simple form builder for Laravel, using Bulma css",
    "authors": [
        {
            "name": "Taufik Rahman",
            "email": "cahsowan18@gmail.com"
        }
    ],
    "require": {
    	"php": ">=7.1.3"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "RealCahsowan\\LaravelBulmaForm\\": "src/"
        },
        "files": [
            "resources/helpers/form.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealCahsowan\\LaravelBulmaForm\\FormBuilderServiceProvider"
            ]
        }
    }
}
